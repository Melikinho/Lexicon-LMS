@model UpLoadDocumentViewModel

@{
    ViewData["Title"] = "FileUpload";
}
<form enctype="multipart/form-data" asp-action="FileUpload">
    <div class="form-group">
        <div>
            <div class="col-md-10">
            <p>Upload File</p>
            <input type="file" asp-for="File" />
        </div>
    </div>
    <div class="form-group">
            <input type="submit" value="Upload" />
    </div>
    <div class="form-group">
            <span>@TempData["msg"]</span>
    </div>
    </div>
</form>